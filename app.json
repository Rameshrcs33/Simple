{
  "expo": {
    "name": "Simple",
    "slug": "Simple",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "simple",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need access to your location to display it on the map",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need access to your location to display it on the map",
        "NSLocationAlwaysUsageDescription": "We need access to your location to display it on the map",
        "NSCameraUsageDescription": "We need camera access for taking photos",
        "MinimumOSVersion": "13.0",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.simple.learning"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.Simple"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1035806a-405c-4210-a4aa-82ea63ad1292"
      }
    }
  }
}
